<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity ServiceOrder.
    -->
    <changeSet id="20250305052920-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="service_order"
                                 constraintName="fk_service_order__type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_order_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="vehicle_id"
                                 baseTableName="service_order"
                                 constraintName="fk_service_order__vehicle_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vehicle"
                                 />

        <addForeignKeyConstraint baseColumnNames="service_order_id"
                                 baseTableName="rel_service_order__products"
                                 constraintName="fk_rel_service_order__products__service_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_order"
                                 />

        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="rel_service_order__products"
                                 constraintName="fk_rel_service_order__products__products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"
                                 />
    </changeSet>
</databaseChangeLog>
